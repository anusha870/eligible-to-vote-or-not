#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */
using namespace std;
int main() {
	int age;
	cout<<"enter your age:";
	cin>>age;
	if(age>=18){
		cout<<"person is eligible for vote"<<"\n";
	}
	else{
		cout<<"person is not eligible for vote"<<"\n";
	}
	return 0;
}
